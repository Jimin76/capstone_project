
<div id="main">
    <div id="content">
        <% rows.forEach(row => { %>
            <div id="reserve"><a href="/">
                <h3>Date: <%= row.Date %></h3>
                <p>Number of customers: <%= row.CusNum %></p>
                <p>Status: <%= row.Status %></p>
            </a>
            <button>Confirm</button>
            <button>Decline/Cancel</button>
            </div>
        <% }) %>
    </div>


</div>

